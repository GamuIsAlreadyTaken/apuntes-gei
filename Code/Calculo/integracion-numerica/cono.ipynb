    "import sympy as sp\n",
    "import numpy as np\n",
    "\n",
    "h, R, x = sp.symbols('h,R,x', real=True)\n",
    "\n",
    "_l = R/h * x\n",
    "\n",
    "print(\"Area del cono: \")\n",
    "display(sp.pi * sp.integrate(_l * _l, (x, 0, h)))\n",
    "\n",
    "\n",
    "l = sp.Lambda(x, _l)\n",
    "\n",
